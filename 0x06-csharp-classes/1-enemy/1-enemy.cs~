using System;

namespace Enemies
{
    /// <summary>
    /// Define the enemie Zombie
    /// </summary>
    class Zombie
    {
        /// <value>The health of the zombie</value>
        public int health;

        /// <summary>
        /// Constructor for Zombie, set health at 0
        /// </summary>
        public Zombie()
        {
            this.health = 0;
        }
    }
}
